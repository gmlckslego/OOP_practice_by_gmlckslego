#include <iostream>
using namespace std;

void swap(int x, int y);
void main() {
    int a = 10, b = 20;
    cout << " a = " << a << " b = " << b << endl; // a = 10 b = 20
    swap(a, b); // a = 20 b = 10
    cout << " a = " << a << " b = " << b << endl; // a = 10 b = 20
}

void swap(int a, int b) {
    int t;
    t = a;
    a = b;
    b = t;
    cout << " a = " << a << " b = " << b << endl;
}
